						getDistance(deliverysLists[this.index],deliverys[this.index],-55)
					}
					deliverys[i].onmouseout=function(){
						changeMode(deliverysLists[this.index],"none");
					}
					deliverysLists[i].onmouseover=function(){
						changeMode(deliverysLists[this.index],"block");
					}
					deliverysLists[i].onmouseout=function(){
						changeMode(deliverysLists[this.index],"none");
					}
				}
				
				function changeMode(elem,mode){
					elem.style.display=mode;
				}
				function getDistance(elem,parEle,dis){
					//offsetLeft:元素距离左边的距离
					elem.style.left=parEle.offsetLeft+dis+"px";
					//offsetTop:元素距离上边的距离
					elem.style.top=parEle.offsetTop+parEle.offsetHeight+"px";
				}
			</script>
			<!--删除订单-->
			<script type="text/javascript" src="js/removeNode.js"></script>
			<script>
				var removeOrder=$(".order_remove");
				var orderlists=$(".finished");
