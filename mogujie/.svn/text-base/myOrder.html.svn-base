			<!--container左边title部分的打开和收起-->
			<!--默认当前页面的cl_nav标签有clickTitle,当点击某title时，添加clickTitle,再次点击取消clickTitle-->
			<script type="text/javascript">
				window.onload=function(){
//					获得可以展开的Titlediv
					var clTitles=$(".has_sma_nav");
//					使用循环给每个div添加点击事件
					for(var i=0;i<clTitles.length;i++){
						clTitles[i].index=i;
						clTitles[i].onclick=function(){
							clTitle(this.index);	
						}
					}
//					点击后需要执行的函数
					function clTitle(num){
						var thisClNav=clTitles[num];
//						判断当前Titlediv的父标签是否具有属性clickTitle 
						var hasClickTitle=thisClNav.parentNode.className.indexOf("clickTitle");
						if(hasClickTitle>-1){
							thisClNav.parentNode.className=thisClNav.parentNode.className.replace("clickTitle","");
						}else{
							thisClNav.parentNode.className+=" clickTitle";
						}
					}
				}
			</script>
