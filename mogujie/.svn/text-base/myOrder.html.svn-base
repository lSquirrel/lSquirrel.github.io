					document.body.appendChild(centerDiv);
					this.clo=function(){
						console.log(bigDiv.parentNode)
						bigDiv.parentNode.removeChild(bigDiv);
						centerDiv.parentNode.removeChild(centerDiv);
					}
					this.ok=function(){
						removeNode(orderlists,num,removeOrder);
						clo();
					}
				}
