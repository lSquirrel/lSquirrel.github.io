		<script type="text/javascript">
			//实现fixdTop的出现、隐藏效果
			var fixdTop=$("#fixdTop");
			document.onscroll=function(){
				if(document.body.scrollTop>=120){
					move(fixdTop,{top:0});
				}
				if(document.body.scrollTop<120){
					move(fixdTop,{top:-50});
				}
			}
			//实现历史记录的出现和隐藏
			var searchBoxs=$(".searchBox");
			var isOnLocal=false;
			for(var i=0;i<searchBoxs.length;i++){
				searchBoxs[i].index=i;
				searchBoxs[i].onfocus=function(){
					ftsbHide(this.index,"block");
				}
				searchBoxs[i].onblur=function(){
					if(!isOnLocal){
						ftsbHide(this.index,"none");
					}
				}
			}
			var ftsbHides=$(".ftsb_hide");
			for(var j=0;j<ftsbHides.length;j++){
				ftsbHides[j].index=j;
				ftsbHides[j].onmouseover=function(){
					isOnLocal=true;
				}
				ftsbHides[j].onmouseout=function(){
					isOnLocal=false;
				}
				ftsbHides[j].onclick=function(){
					$(".searchBox")[this.index].focus();
					ftsbHide(this.index,"block");
					isOnLocal=false;
				}
			}
			function ftsbHide(objindex,display){
				ftsbHides[objindex].style.display=display;
			}
			//实现点击删除历史记录
			var localDel=$(".ftsb_hide")[0].getElementsByClassName("local_del");//x号
			for(var i=0;i<localDel.length;i++){
				localDel[i].index=i;
				localDel[i].onclick=function(){
					delLocal(this.index);
				}
			}
			function delLocal(num){
				$(".local_item")[num].style.display="none";
			}
			$(".del_all")[0].onclick=function(){
				$(".local_item_ul")[0].parentNode.removeChild($(".local_item_ul")[0]);
				this.style.display="none";
			}
		</script>
		
