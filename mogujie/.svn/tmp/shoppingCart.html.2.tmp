				}
				$("#myCollect").onmouseout=function(){
					changeMode(myCollect_child,'none');
				}
				$("#myCollect_child").onmouseover=function(){
					changeMode(myCollect_child,'block');
				}
				$("#myCollect_child").onmouseout=function(){
					changeMode(myCollect_child,'none');
				}
				$("#customerSevice").onmouseover=function(){
					changeMode(customerSevice_child,'block');
					getDistance(customerSevice_child,this);
				}
				$("#customerSevice").onmouseout=function(){
					changeMode(customerSevice_child,'none');
				}
				$("#customerSevice_child").onmouseover=function(){
					changeMode(customerSevice_child,'block');
				}
				$("#customerSevice_child").onmouseout=function(){
					changeMode(customerSevice_child,'none');
				}
				$("#myStore").onmouseover=function(){
					changeMode(myStore_child,'block');
					getDistance(myStore_child,this);
				}
				$("#myStore").onmouseout=function(){
					changeMode(myStore_child,'none');
				}
				$("#myStore_child").onmouseover=function(){
					changeMode(myStore_child,'block');
				}
				$("#myStore_child").onmouseout=function(){
					changeMode(myStore_child,'none');
				}
				$("#shopping").onmouseover=function(){
					changeMode(shopping_child,'block');
					getDistance(shopping_child,this);
				}
				$("#shopping").onmouseout=function(){
					changeMode(shopping_child,'none');
				}
				$("#shopping_child").onmouseover=function(){
					changeMode(shopping_child,'block');
				}
				$("#shopping_child").onmouseout=function(){
					changeMode(shopping_child,'none');
				}

				function changeMode(elem,mode){
					elem.style.display=mode;
				}
				function getDistance(elem,parEle){
					var left=parEle.offsetLeft;
					if(elem.id==="shopping_child"){
						left-=80;
					}
					//offsetLeft:元素距离左边的距离
					elem.style.left=left+"px";
					//offsetTop:元素距离上边的距离
					elem.style.top=parEle.offsetHeight+"px";
				}
				
				var lihovers=document.getElementsByClassName("lihover");
				for(var i=0;i<lihovers.length;i++){
					lihovers[i].index=i;
					lihovers[i].onmouseover=function(){
						lihovers[this.index].style.backgroundColor="#E4E4E4";
					}
					lihovers[i].onmouseout=function(){
						lihovers[this.index].style.backgroundColor="";
					}
				}
			}

		</script>
		
		
		
