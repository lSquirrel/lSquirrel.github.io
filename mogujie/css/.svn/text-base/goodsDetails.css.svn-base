	top:0;
	line-height:24px;
}
.goodsDetail_content .goods_wrap .contentall .goodsinfo_title{
	color:#ef2f23;
}
.goodsDetail_content .goods_wrap .contentall .goodsinfo_close{
	background:url(../img/goodsDetails/goodsbg2_168x30.png) no-repeat -10px -147px;
	display:block;
	width:18px;
	height:18px;
	position:absolute;
	top:5px;
	right:0;
	cursor:pointer;
	color:#666;
	font-size:18px;
}
.goodsDetail_content .goods_wrap .contentall .content{
	padding:5px 10px 0;
	margin-top:30px;
	overflow:hidden;
}
.goodsDetail_content .goods_wrap .contentall .content ol.style{
	width:85%;
}
.goodsDetail_content .goods_wrap .contentall .goodsinfo_action{
	display:none;
	padding:0 0 10px 60px;
}
.goodsDetail_content .goods_wrap .count{
	margin-bottom:10px;;
}
.goodsDetail_content .goods_wrap .contentall .goodsinfo_action .action_ok{
	display:block;
	clear:both;
	height:40px;
	width:120px;
	background:#ef2f23;
	color:#fff;
	font-size:20px;
	line-height:40px;
	text-align:center;
}
.goodsDetail_content .goods_wrap .contentall .goods_buy{
	display:none;
}
.goodsDetail_content .goods_wrap .goods_social{
	margin-top:-40px;
}


#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.style{
	overflow:hidden;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.style li.style_item{
	float:left;
	padding:0;
	margin:0 8px 5px 0;
	line-height:28px;
	position:relative;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.style li.style_item img{
	width:40px;
	height:40px;
	vertical-align:middle;
	border:2px solid transparent;
	cursor:pointer;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.style li.style_item img:hover{
	border-color:#666;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.style li.style_item b{
	position:absolute;
	right:0;
	bottom:0;
	width:11px;
	height:11px;
	background:url(../img/goodsDetails/selected_11x11.png) no-repeat 100% 100%;
	display:none;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.style li.style_item.select b{
	display:block;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.style li.style_item.select img{
	border-color:#666;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.size{
	overflow:hidden;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.size li.size_item{
	float:left;
	cursor:pointer;
	padding:0 10px;
	font-weight:700;
	line-height:28px;
	border:1px solid #ddd;
	margin:0 8px 5px 0;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.size li.size_item:hover{
	border-color:#666;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content ol.size li.size_item.select{
	background:url(../img/goodsDetails/selected_11x11.png) no-repeat 100% 100%;
	border:2px solid #666;
