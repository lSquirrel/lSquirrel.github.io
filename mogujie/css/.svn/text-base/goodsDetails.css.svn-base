	width:60px;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main dd{
	float:left;
	font-size:14px;
	color:#999;
	height:32px;
	line-height:32px;
	margin-bottom:5px;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main dd.origin_price{
	text-decoration:line-through;
	font-family:"微软雅黑",arial;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main dd.now_price{
	color:#ef2f23;
	font-size:24px;
	font-weight:700;
	vertical-align:middle;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main dd.price_extra{
	float:right;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main dd.price_extra span{
	margin-right:10px;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main dd.price_extra span.num{
	color:#333;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content{
	margin-top:15px;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content li{
	overflow:hidden;
	padding-left:10px;	
	padding-bottom:15px;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content li span.item_title{
	color:#999;
	font-size:12px;
	display:inline-block;
	width:60px;
}
#container .shop_detail .primary_goods .goods_detail .goodsDetail_content li .contact_talk{
	width:85px;
	height:24px;
	background:url(../img/goodsDetails/concat_talk.gif) no-repeat center;
	display:inline-block;
	vertical-align:middle;
}
.goodsDetail_content .goodsinfo_title{
	display:none;
}
.goodsDetail_content .contentall .goodsinfo_action{
	display:none;
}
.goodsDetail_content  .goods_wrap{
	width:440px;
	margin-right:15px;
	margin-left:-10px;
}
.goodsDetail_content .goods_wrap .contentall{
	border:1px solid #ef2f23;
	position:relative;
	z-index:100;
	background-color:#fff;
	overflow:hidden;
}
.goodsDetail_content .goods_wrap .contentall .goodsinfo_title{
	width:100%;
	text-indent:10px;
	background:#f6f6f6;
	display:block;
	height:24px;
