.goodsDetail_content .count{
	margin-top:10px;
	overflow:hidden;
} 
.goodsDetail_content .count .numCount #goosNum{
	width:95px;
	height:25px;
	border:1px solid #ddd;
	overflow:hidden;
}
.goodsDetail_content .count .numCount #goosNum span.num_reduce,.goodsDetail_content .count .numCount #goosNum span.num_add{
	display:inline-block;
	width:24px;
