.goodsDetail_content li.service_extra .services span img{
	width:24px;
	height:24px;
	margin-right:5px;
}
.goodsDetail_content li.service_extra .services span{
	color:#666;
}
.goodsDetail_content li.service_extra div.payfor{
	width:131px;
	height:20px;
	display:inline-block;
	background:url(../img/goodsDetails/goodsbg2_168x30.png) no-repeat -38px -295px;
}



#container .shop_detail .primary_ad{
	overflow:hidden;
	border-left:1px solid #eee;
	padding-left:20px;
	width:100px;
}
#container .shop_detail .primary_ad p.title{
	color:#666;
	height:14px;
	margin-bottom:10px;
	text-align:center;
	width:100%;
	position:relative;
}
#container .shop_detail .primary_ad p.title s{
	display:inline-block;
	width:100%;
	height:0;
	border-top:1px solid #eee;
}
#container .shop_detail .primary_ad p.title span{
	display:inline-block;
	color:#999;
	text-align:center;
	position:absolute;
	top:0;
