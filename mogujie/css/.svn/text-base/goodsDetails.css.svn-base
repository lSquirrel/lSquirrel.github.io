	background:url(../img/goodsDetails/goodsbg2_168x30.png) no-repeat;
}
.goodsDetail_content .count .numCount #goosNum span.num_reduce{
	background-position:-3px 2px;
}
.goodsDetail_content .count .numCount #goosNum span.num-disable{
	opacity:.3;
