	height:25px;
	border-left:1px solid #ddd;
	border-right:1px solid #ddd;
}
.goodsDetail_content .count .numCount #goosNum span.num_add{
	background-position:-3px -78px;
}
.goodsDetail_content .count .numCount .goods_count{
	height:25px;
	line-height:25px;
	margin-left:10px;
	color:#999;
}
