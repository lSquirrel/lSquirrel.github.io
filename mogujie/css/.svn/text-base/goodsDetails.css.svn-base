body{
	background: #fff; /*防止用户修改自己的操作系统默认的颜色造成的影响*/
	font:12px/1.6 tahoma,arial,sans-serif;
	color:#333;
}
a{
	color:#333;
}
a.underline:hover{
	text-decoration:underline;
}
a.underlinenow{
	text-decoration:underline;
	color:#666;
}
.linethrough{
	text-decoration:line-through;
}
.lt{
	float:left;
}
.rt{
	float:right;
}
.clear{
	clear:both;
}
.wrapall{
	width:100%;
	min-width:1000px;
}

.wrap{
	width:1000px;
	height:100%;
	margin-left:auto;
	margin-right:auto;
	overflow:hidden;
}



#shop_info{
	border-bottom:1px solid #dedede;
}
#shop_info .shopInfo_left{
	width:505px;
	overflow:hidden;
	cursor:pointer;
}
#shop_info .shopInfo_left.hover{
	background:#f5f5f5;
}
#shop_info .shopInfo_left a.shoplogo img{
	width:40px;
	height:40px;
	border-radius:50%;
	margin-left:20px;
	margin-top:15px;
}
#shop_info .shopInfo_left div.shop_name{
	padding-left:10px;
	margin-top:15px;
	width:240px;
}
#shop_info .shopInfo_left div.shop_name a.name{
	margin-right:5px;
	display:inline-block;
	max-width:130px;    /*该样式可以适用于任何商品详情页*/
	overflow:hidden;
	color:#333;
	white-space:nowrap;
	word-break:break-all;
	text-overflow:ellipsis;
	line-height:20px;
	font-size:14px;
	font-weight:400;
}
#shop_info .shopInfo_left div.shop_name span.mark{
	display:inline;
	height:20px;
	line-height:20px;
}
#shop_info .shopInfo_left div.shop_name span.mark img{
	height:16px;
	vertical-align:2px;
}
#shop_info .shopInfo_left div.shop_name p.shop_score{
	overflow:hidden;
}
#shop_info .shopInfo_left div.shop_name p.shop_score span.scoreItem{
	color:#666;
	margin-right:5px;
}
#shop_info .shopInfo_left div.shop_name p.shop_score span.scoreItem b{
	color:#ef2f23;
	padding-left:2px;
}
#shop_info .shopInfo_left div.shopinfo_arrow{
	width:40px;
	height:46px;
	padding-top:20px;
	margin-left:3px;
	cursor:pointer;
	background:url(../img/goodsDetails/shopinfo_arrow_7x4.png) no-repeat center;
}
#shop_info .shopInfo_left.hover div.shopinfo_arrow{
	background:url(../img/goodsDetails/shopinfo_arrowup_7x4.png) no-repeat center;
}
#shop_info .shopInfo_left div.shop_action{
	margin-top:24px;
	overflow:hidden;
}
.header_icons{
	background:url(../img/goodsDetails/goodsbg_168x30.png) no-repeat;
}
#shop_info .shopInfo_left div.shop_action a{
	display:block;
	float:left;
	height:20px;
	line-height:20px;
	background-color:#fff;
	border:1px solid #ddd;
}
#shop_info .shopInfo_left div.shop_action a.shop_collect{
	padding:0 12px 0 22px;
	background-position:4px -48px;
	font-size:12px;
	color:#666;
	margin-right:10px;
}
#shop_info .shopInfo_left div.shop_action a.chart{
	padding:0 5px;
	width:48px;
	background:#fff url(../img/goodsDetails/chart.gif) no-repeat center ;
}
#shop_info div.shopInfo_list{
	position:absolute;
	background:#f6f6f6;
	border-top:0;
	width:505px;
	overflow:hidden;
	line-height:26px;
	display:none;
	border-top:1px solid #dedede;
	z-index:20;
}
#shop_info div.shopInfo_list ol.info_box{
	width:220px;
	min-height:250px;
	overflow:hidden;
	border-right:1px solid #dedede;
}
#shop_info div.shopInfo_list ol.ol{
	padding:5px 0;
}
#shop_info div.shopInfo_list ol.ol1{
	width:150px;
	margin:0 0 15px 15px;
}
#shop_info div.shopInfo_list ol.ol .tag{
	display:inline-block;
	width:85px;
	text-align:right;
	color:#999;
	margin-right:2px;
}
#shop_info div.shopInfo_list ol li.btn{
	text-align:center;
	margin-top:10px;
}
#shop_info div.shopInfo_list ol li a.goodDetail_btn{
	display:inline-block;
	border:1px solid #d5d5d5;
	background-color:#fff;
	color:#333;
	width:100px;
	height:26px;
	line-height:26px;
	text-align:center;
	border-radius:3px;
	margin-top:10px;
}
#shop_info div.shopInfo_list ol .Score_num{
	color:#f36;
}
#shop_info div.shopInfo_list ol.ol2{
	width:80px;
}
#shop_info div.shopInfo_list ol.ol2 .up{
	padding:2px;
	background-color:#f36;
	margin-right:5px;
	color:#fff;
}
#shop_info div.shopInfo_list ol.ol2 .Score_num{
	color:#333;
}
#shop_info div.shopInfo_list ol.ol3{
	width:150px;
	margin-left:15px;
	padding:5px 0;
	border-top:1px dashed #dedede;
}
#shop_info div.shopInfo_list ol.ol4{
	width:70px;
	padding:5px 0;
	border-top:1px dashed #dedede;
}



#shop_info .shopInfo_right{
	margin:18px 0 0;
	padding-left:24px;
	overflow:hidden;
}
#shop_info .shopInfo_right .shopInfo_search{
	overflow:hidden;
}
#shop_info .shopInfo_right .shopInfo_search input.search_text{
	background:#fff;
	width:295px;
	height:26px;
	line-height:26px;
	border:2px solid #ef2f23;
	padding:0 8px;
	color:#999;
	outline:none;
}
#shop_info .shopInfo_right .shopInfo_search input.submit_btn{
	width:72px;
	height:30px;
	border:0;
	background-color:#ef2f23;
	color:#fff;
	cursor:pointer;
}
#shop_info .shopInfo_right .shopInfo_search input.search_inshop{
	width:72px;
	height:30px;
	line-height:28px;
	border:1px solid #ef2f23;
	background-color:#fff;
	margin-left:5px;
	color:#ef2f23;
	cursor:pointer;
}

#shop_info .shopInfoRight_list{
	position:absolute;
	background-color:#fff;
	border:1px solid #cecece;
	z-index:999;
	width:464px;
	font-size:12px;
	min-height:286px;
	margin-top:1px;
	display:none;
}
#shop_info .shopInfoRight_list h3{
	padding:10px;
}
#top_banner{
	padding:20px;
	height:90px;
	overflow:hidden;
}
#top_banner a{
	display:block;
	height:110px;
	background:url(../img/goodsDetails/top_banner.jpg) no-repeat center;
	background-size:1400px;
}


#shop_nav{
	height:30px;
	background:#000;
	background:rgba(0,0,0,.75);
	position:relative;
	z-index:19;
	margin-bottom:20px;
}
#shop_nav ul{
	height:30px;
	overflow:hidden;
}
#shop_nav ul li{
	float:left;
	font-size:14px;
	padding:7px 0;
}
#shop_nav ul li a{
	display:block;
	color:#fff;
	padding:0 20px;
	line-height:16px;
	border-right:1px solid #5c5c5c;
}
#shop_nav ul li.all a{
	padding:0 38px 0 25px;
	background:url(../img/goodsDetails/goodsbg1_200x500.png) no-repeat 86px -191px;
}
#shop_nav ol.category_list{
	position:absolute;
	width:150px;
	line-height:32px;
	background:#000;
	background:rgba(0,0,0,.75);
	display:none;
}
#shop_nav ol.category_list li.subMenu{
	height:32px;
	line-height:32px;
	overflow:visible;
}
#shop_nav ol.category_list li.subMenu a{
	color:#fff;
	font-size:12px;
	height:32px;
	line-height:32px;
	padding:0 26px;
	background:url(../img/goodsDetails/goodsShopNavAll_4x7.png) no-repeat 14px 5px;
}

#container .shop_detail{
	overflow:hidden;
	margin:30px 0;
	background-color:#fff;
	min-height:570px;
}
#container .shop_detail .primary_goods{
	width:870px;
	overflow:hidden;
}
#container .shop_detail .primary_goods .goods_topImg .big_img{
	width:350px;
	height:525px;
	overflow:hidden;
	background-color:#fff;
}
#container .shop_detail .primary_goods .goods_topImg .big_img img{
	width:350px;
	height:525px;
}
#container .shop_detail .primary_goods .goods_topImg .small_img{
	position:relative;
	margin-top:10px;
	padding:0 25px;
	width:300px;
}
#container .shop_detail .primary_goods .goods_topImg .small_img .img_box{
	width:310px;
	height:55px;
	overflow:hidden;
}
#container .shop_detail .primary_goods .goods_topImg .small_img .img_box ul.img_list{
	width:9999px;
	position:relative;
	left:0;
}
#container .shop_detail .primary_goods .goods_topImg .small_img .img_box ul.img_list li{
	float:left;
	width:55px;
	opacity:0.5;
	filter:alpha(opacity:50);
	margin-right:8px;
	cursor:pointer;
	position:relative;
}
#container .shop_detail .primary_goods .goods_topImg .small_img .img_box ul.img_list li.chose{
	opacity:1;
	filter:alpha(opacity:100);
}
#container .shop_detail .primary_goods .goods_topImg .small_img .img_box ul.img_list li i{
	height:2px;
	width:55px;
	position:absolute;
	left:0;
	bottom:0;
	background:#ef2f23;
	display:none;
}
#container .shop_detail .primary_goods .goods_topImg .small_img .img_box ul.img_list li.chose i{
	display:block;
}
#container .shop_detail .primary_goods .goods_topImg .small_img .img_box ul.img_list li img{
	width:55px;
}
#container .shop_detail .primary_goods .goods_topImg .small_img a.arraw_Btn{
	background:url(../img/goodsDetails/goodsbg2_168x30.png) no-repeat;
	width:12px;
	height:22px;
	position:absolute;
	top:17px;
}
#container .shop_detail .primary_goods .goods_topImg .small_img a.left_Btn{
	display:none;
	left:0;
	background-position:-10px -256px;
	overflow:hidden;
}
#container .shop_detail .primary_goods .goods_topImg .small_img a.right_Btn{
	display:block;
	right:0;
	background-position:-10px -217px;
	overflow:hidden;
}
#container .shop_detail .primary_goods .goods_detail{
	width:500px;
	margin:0 0 0 20px;
	overflow:hidden;
}
#container .shop_detail .primary_goods .goods_detail h1.goods_name{
	font-family:"微软雅黑",arial;
	color:#333;
	margin-bottom:15px;
	font-size:24px;
	line-height:25px;
	font-weight:900;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main{
	width:100%;
	border-bottom:1px solid #f3f3f3;
	border-top:1px solid #f3f3f3;
	background:url(../img/goodsDetails/pricebg_540x163.png) repeat-y  0 0;
	margin-left:-10px;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main dl{
	padding-left:10px;
	overflow:hidden;
}
#container .shop_detail .primary_goods .goods_detail .goodsPrice_main dt{
	color:#999;
	font-size:12px;
	height:32px;
	line-height:32px;
