}
.ali_stitle2{
	width: 98px;
	height: 20px;
	top: 66px;
	font-size: 14px;
}
.ali_goicon{
	position: absolute;
	bottom: 18px;
	left: 20px;
	width: 50px;
	height: 24px;
	z-index: 3;
}
.ali_goicon img{
	width: 100%;
	display: block;
}
.ad_right{
	position: relative;
	width: 166px;
	height: 150px;
	overflow: hidden;
	border-radius: 6px;
	float: left;
}
.ad_right img{
	width: 100%;
}
.ad_right .arimg_hover{
	display: none;
}

