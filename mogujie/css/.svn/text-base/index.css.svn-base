.ad_2{
	background: #00dafe;
	background: linear-gradient(left,#00dafe,#3381ff);
	background: -webkit-linear-gradient(left,#00dafe,#3381ff);
}
.ad_3{
	background: #fd3f89;
	border-radius: 0 6px 6px 0;
